$nbre = 7;

//   FOR)
for ($i = 1; $i <= $nbre; $i++){
    echo $i. "<br>";
}



echo "<br>";



//   WHILE)
$x = 1;
while ($x <= $nbre){
    echo $x. "<br>";
    $x++;
}
